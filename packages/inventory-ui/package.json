{
	"name": "@brittonhayes/inventory-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"gen:api-client": "npx dtsgenerator --url http://localhost:5000/docs-json -o ./src/lib/types/inventory-api.d.ts",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write . && eslint . --fix",
		"typesafe-i18n": "typesafe-i18n"
	},
	"type": "module",
	"dependencies": {
		"@dicebear/collection": "^5.0.2",
		"@dicebear/core": "^5.0.2",
		"@tiptap/starter-kit": "^2.0.0-beta.209",
		"axios": "^1.2.3",
		"es6-promise": "^4.2.4",
		"material-symbols": "^0.4.2",
		"typesafe-i18n": "^5.18.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-node": "^1.1.4",
		"@sveltejs/adapter-static": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@tailwindcss/typography": "^0.5.8",
		"@types/feather-icons": "^4.29.1",
		"@types/url-parse": "1.4.4",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.7",
		"daisyui": "^2.46.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"material-icons": "^1.13.1",
		"postcss": "^8.4.14",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	}
}
